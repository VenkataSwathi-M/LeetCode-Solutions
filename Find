class Solution(object):
    def strStr(self, haystack, needle):
        """
        :type haystack: str
        :type needle: str
        :rtype: int
        haystack = "sadbutsad", needle = "sad"
        haystack = "leetcode", needle = "leeto"
        """
        x = len(haystack)
        if needle in haystack:
            return haystack.index(needle)
        else:
            return -1
solu = Solution()
print(solu.strStr("sadbutsad","sad"))
        